{
  "name": "@oceanopen/eslint-config-antfu",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@8.15.9",
  "description": "Eslint config for antfu/eslint-config",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ocean-gao/eslint-config-antfu.git"
  },
  "bugs": "https://github.com/ocean-gao/eslint-config-antfu/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "scripts": {
    "prepare": "simple-git-hooks",
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "pnpm run dev && eslint .",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && npm publish",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": "*"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.10"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@types/node": "^22.5.5",
    "bumpp": "^9.5.2",
    "eslint": "^9.12.0",
    "lint-staged": "^15.2.10",
    "pnpm": "^9.11.0",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "unbuild": "^2.0.0",
    "vite": "^5.4.6"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
